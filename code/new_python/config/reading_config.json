            "file_patterns": [
                "ipf16.csv",
                "ipf36.csv"
            ],
            "dynamic_columns": [
                {
                    "pattern": "*16*",
                    "columns": {
                        "cdpole": "1"
                    }
                },
                {
                    "pattern": "*36*",
                    "columns": {
                        "cdpole": "3"
                    }
                }
            ],
            "schema": "ipf_az",
            "read_options": {
                "format": "csv",
                "sep": "|",
                "header": true,
                "encoding": "LATIN9",
                "dateFormat": "dd/MM/yyyy"
            },
            "filters": [],
